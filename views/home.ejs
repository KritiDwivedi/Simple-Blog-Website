<%-include("partials/header")-%>
<h1><%=heading%></h1>
<p><%=content%></p>
<!-- TAKING OUT TITLE FROM THE object ARRAY -->
<%showing.forEach(function(item){%>
    
    <div class="title">
<h3><p><%=item.title;%></p></h3></div>


<div class="content">
    <p><%=item.content.substring(0,100)+"..."%><a href="/post/<%= item.title %>">Read More</a></p>
    <p></p> 
    
   
<%})%>

